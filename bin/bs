$hash {'documentation'} = 'https://9ci.svn.beanstalkapp.com/documentation';
$hash {'dynamicsax'} = 'https://9ci.svn.beanstalkapp.com/9ci-plugins/grails-dynamicsax';
$hash {'gb-dev-docs'} = 'https://9ci.svn.beanstalkapp.com/gb_dev_docs';
$hash {'gb-quickbooks'} = 'https://9ci.svn.beanstalkapp.com/gb_quickbooks';
$hash {'gb-selenium'} = 'https://9ci.svn.beanstalkapp.com/gb_selenium';
$hash {'greenbill'} = 'https://9ci.svn.beanstalkapp.com/greenbill';
$hash {'payment-gateway'} = 'https://9ci.svn.beanstalkapp.com/9ci-plugins/grails-payment-gateway';
$hash {'quickbooks'} = 'https://9ci.svn.beanstalkapp.com/9ci-plugins/grails-quickbooks';
$hash {'rally'} = 'https://9ci.svn.beanstalkapp.com/9ci-plugins/grails-rally';
$hash {'sparkle'} = 'https://9ci.svn.beanstalkapp.com/9ci-plugins/grails-sparkle';
